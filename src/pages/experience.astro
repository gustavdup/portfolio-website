---
import Layout from '../layouts/Layout.astro';
import ExperienceListReact from '../components/ExperienceListReact.jsx';
import { getCollection } from 'astro:content';

const experienceProductStrategistExperiences = await getCollection('experienceProductStrategist');
const experienceExecutionLeadExperiences = await getCollection('experienceExecutionLead');
const experienceDigitalEnablerExperiences = await getCollection('experienceDigitalEnabler');
const experienceFractionalPmExperiences = await getCollection('experienceFractionalPm');
const experienceCollaborativeLeaderExperiences = await getCollection('experienceCollaborativeLeader');
const experienceCommercialStrategistExperiences = await getCollection('experienceCommercialStrategist');
const experienceStrategicTechnologistExperiences = await getCollection('experienceStrategicTechnologist');
---

<Layout>
  <ExperienceListReact 
    client:load 
    experienceProductStrategistExperiences={experienceProductStrategistExperiences}
    experienceExecutionLeadExperiences={experienceExecutionLeadExperiences}
    experienceDigitalEnablerExperiences={experienceDigitalEnablerExperiences}
    experienceFractionalPmExperiences={experienceFractionalPmExperiences}
    experienceCollaborativeLeaderExperiences={experienceCollaborativeLeaderExperiences}
    experienceCommercialStrategistExperiences={experienceCommercialStrategistExperiences}
    experienceStrategicTechnologistExperiences={experienceStrategicTechnologistExperiences}
  />
</Layout>
